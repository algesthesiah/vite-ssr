{
  "locales": [
    "en",
    "zh"
  ],
  "sourceLocale": "en",
  "compileNamespace": "ts",
  "catalogs": [
    {
      "path": "locales/{locale}/messages",
      "include": [
        "components",
        "modules",
        "helper",
        "renderer"
      ]
    }
  ],
  "format": "po",
  "fallbackLocales": {
    "default": "en"
  },
  "extractBabelOptions": {
    "presets": [
      "@babel/preset-typescript"
    ]
  }
}
