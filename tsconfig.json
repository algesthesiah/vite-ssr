{
  "extends": "base-ts-config",
  "compilerOptions": {
    "composite": false,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    },
       "jsx": "preserve",
    "target": "es2016"
  },
  "types": ["vite/client", "react/next", "react-dom/next"],
  "include": ["./**/*", "./package.json"],
  "exclude": ["node_modules", "assets", "dist"],
  "extensions": [".ts", ".tsx", ".js", ".jsx"]
}
