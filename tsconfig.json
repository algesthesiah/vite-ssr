{
  "extends": "base-ts-config",
  "compilerOptions": {
    "jsx": "react-jsxdev",
    "baseUrl": "./src",
    "paths": {
      "~": [
        "./src"
      ],
      "~/*": [
        "./src/*"
      ]
    }
  },
  "include": ["src/**/*", "types/*", "*"],
  "exclude": [
    "node_modules",
    "assets",
    "dist"
  ]
}